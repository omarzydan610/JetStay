type Airline {
    id: ID
    name: String
    price: Float
    rating: Float
    country: String
}

input AirlineFilter {
    priceLt: Float
    priceGt: Float
    ratingGte: Float
    ratingLte: Float
    country: String
    nameContains: String
}

type Query {
    airlines(filter: AirlineFilter): [Airline]
}